
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link href="~/css/table.css" rel="stylesheet" />
    <link href="~/javaScript/calendar/calendar-win2k-cold-1.css" rel="stylesheet" />
    <script src="~/javaScript/calendar/cal.js"></script>
    <script src="~/javaScript/comm/comm.js"></script>
    <script src="~/javaScript/comm/list.js"></script>
    <script src="~/javaScript/calendar-ch.js"></script>
    <script src="~/javaScript/comm/time.js"></script>
    <link href="~/css/cwcalendar.css" rel="stylesheet" />
    <script src="~/javaScript/jquery-1.7.2.js"></script>
    <script src="~/javaScript/vue-2.4.0.js"></script>
</head>
<body>
    <div id="div11">

        <input type="hidden" name="page.startPage" value="0">
        <table width="100%">
            <tr>
                <td>
                    <font color="black">您正在做的业务是:人力资源管理--薪酬标准管理--薪酬标准变更</font>
                </td>
            </tr>
            <tr>
                <td>
                    <div align="right">
                        <input type="button" value="查询" class="BUTTON_STYLE1" v-on:click="tj()" />
                    </div>
                </td>
            </tr>
            </tr>
        </table>
        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE2">
            <tr>
                <td width="20%" class="TD_STYLE1">
                    请输入薪酬标准编号
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="standard.standardId" value="" id="tid"
                           class="INPUT_STYLE1">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    请输入关键字
                </td>
                <td class="TD_STYLE2">
                    <input type="text" name="utilbean.primarKey" class="INPUT_STYLE1" id="tname">
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    请输入建档时间
                </td>
                <td width="84%" class="TD_STYLE2">
                    <input name="utilbean.startDate" onclick="aa('utilbean.startDate')" type="text" class="INPUT_STYLE2" id="time1" style="width:24% " value="" size="30">
                    至 <input type="text" name="utilbean.endDate" onclick="aa('utilbean.endDate')" value="" style="width:24% " class="INPUT_STYLE2" id="time2">
                    （YYYY-MM-DD）
                </td>
            </tr>
        </table>

    </div>
    <script type="text/javascript">
        var vm = new Vue({
            el: "#div11",
            data: {
                dt: [],
                pages: 1,
                row: 0,
                currentpage: 1
            },
            methods: {
                tj: function () {

                    if ($("#tid").val() == "" && $("#tname").val() == "" && $("#time1").val() == "" && $("#time2").val()=="") {
                        alert("请至少输入一个条件");
                    } else {
                        var id =$("#tid").val();
                        var name = $("#tname").val();
                        var time1 = $("#time1").val();
                        var time2 = $("#time2").val();
                        var zong = id + "," + name + "," + time1 + "," + time2;
                        location.href = "/Update/Index2/" +zong + "";
                    }
                }
            },
            mounted: function () {

            }
        });
    </script>
</body>
</html>
